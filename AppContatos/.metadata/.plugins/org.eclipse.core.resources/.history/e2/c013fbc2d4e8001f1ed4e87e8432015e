package br.com.dnl.AppContatos.Enum;

public enum OrderTipo {
    TELEFONE_RESIDENCIAL(1),
    CELULAR(2),
    WHATSAPP(3),
    EMAIL_PESSOAL(4),
    EMAIL_PROFISSIONAL(5);

    private final int tipo;

    OrderTipo(int tipo) {
        this.tipo = tipo;
    }

    public int getTipo() {
        return tipo;
    }

    public static OrderTipo fromTipo(int tipo) {
        for (OrderTipo value : OrderTipo.values()) {
            if (tipo == value.getTipo()) {
                return value;
            }
        }
        throw new IllegalArgumentException("Código inválido para OrderTipo: " + tipo);
    }
}
